{
  "name": "@spikers/next-openapi-json-generator",
  "version": "2.0.5",
  "description": "a Next.js plugin to generate OpenAPI documentation from route handlers",
  "keywords": [
    "next.js",
    "swagger",
    "openapi",
    "swagger.json",
    "openapi.json",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqikerz/next-openapi-json-generator.git"
  },
  "bugs": {
    "url": "https://github.com/sqikerz/next-openapi-json-generator/issues"
  },
  "homepage": "https://github.com/sqikerz/next-openapi-json-generator#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "prettier --check './src/**/*.{js,jsx,mjs,cjs,ts,tsx,json}' --plugin=prettier-plugin-organize-imports",
    "lint:fix": "prettier --write './src/**/*.{js,jsx,mjs,cjs,ts,tsx,json}' --plugin=prettier-plugin-organize-imports",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "@omer-x/json-schema-types": "^1.0.4",
    "@spikers/next-openapi-route-handler": "^2.0.1",
    "@omer-x/openapi-types": "^1.3.0",
    "typescript": "^5.9.3",
    "zod": "^4.2.1"
  },
  "dependencies": {
    "@omer-x/openapi-optimizer": "alpha",
    "@omer-x/package-metadata": "^1.0.2",
    "minimatch": "^10.2.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "semantic-release": "^25.0.3",
    "tsup": "^8.5.1",
    "vitest": "^4.0.18"
  }
}
